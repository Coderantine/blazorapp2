{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "terminal.css@0.7.1",
      "destination": "wwwroot/lib/terminal.css/",
      "files": [
        "dist/terminal.css",
        "dist/terminal.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@4.4.1",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap-grid.css",
        "dist/css/bootstrap-grid.min.css"
      ]
    },
    {
      "library": "normalize@8.0.1",
      "destination": "wwwroot/lib/normalize/"
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap-utilities@4.1.3",
      "destination": "wwwroot/lib/bootstrap-utilities/",
      "files": [
        "bootstrap-utilities.css",
        "bootstrap-utilities.min.css"
      ]
    }
  ]
}